<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="kubrick" class="com.baeldung.beaninjection.domain.Director">
        <constructor-arg index="0" value="Stanley"/>
        <constructor-arg index="1" value="Kubrick"/>
    </bean>

    <bean id="clockworkorange" class="com.baeldung.beaninjection.domain.Movie">
        <property name="director" ref="kubrick"/>
        <property name="title" value="Clockwork Orange"/>
    </bean>

</beans>
